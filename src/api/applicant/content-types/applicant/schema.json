{
  "kind": "collectionType",
  "collectionName": "applicants",
  "info": {
    "singularName": "applicant",
    "pluralName": "applicants",
    "displayName": "Applicant",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "email": {
      "type": "email",
      "required": true
    },
    "name": {
      "type": "string",
      "required": true
    },
    "age": {
      "type": "integer",
      "required": true
    },
    "role": {
      "type": "string",
      "required": true
    },
    "zip": {
      "type": "biginteger",
      "required": true
    },
    "experience": {
      "type": "string",
      "required": true
    },
    "commitment": {
      "type": "float",
      "required": true
    },
    "accepted": {
      "type": "boolean",
      "required": true
    },
    "reason": {
      "type": "text",
      "required": true
    },
    "project": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::project.project"
    },
    "level": {
      "type": "enumeration",
      "enum": [
        "beginner",
        "intermediate",
        "advanced"
      ],
      "required": true
    },
    "skills": {
      "type": "component",
      "repeatable": true,
      "component": "skills.skills"
    },
    "yearsOfExperience": {
      "type": "decimal",
      "required": true,
      "min": 0,
      "max": 100
    },
    "isAgeOver18": {
      "type": "boolean",
      "default": true,
      "required": true
    },
    "isTermsAgreed": {
      "type": "boolean",
      "default": true,
      "required": true
    },
    "portfolioLink": {
      "type": "string"
    },
    "portfolioFileId": {
      "type": "string"
    },
    "extraInfo": {
      "type": "string"
    }
  }
}
