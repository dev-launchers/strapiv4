{
  "kind": "collectionType",
  "collectionName": "applicants",
  "info": {
    "singularName": "applicant",
    "pluralName": "applicants",
    "displayName": "Applicant",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "email": {
      "type": "email",
      "required": true
    },
    "name": {
      "type": "string",
      "required": true
    },
    "age": {
      "type": "integer",
      "required": true
    },
    "role": {
      "type": "string",
      "required": true
    },
    "zip": {
      "type": "biginteger",
      "required": true
    },
    "experience": {
      "type": "string",
      "required": true
    },
    "commitment": {
      "type": "float",
      "required": true
    },
    "accepted": {
      "type": "boolean",
      "required": true
    },
    "reason": {
      "type": "text",
      "required": true
    },
    "project": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::project.project"
    },
    "level": {
      "type": "enumeration",
      "enum": [
        "beginner",
        "intermediate",
        "advanced"
      ],
      "required": true
    },
    "yearsOfExperience": {
      "type": "decimal",
      "required": true,
      "min": 0,
      "max": 100
    },
    "isAgeOver18": {
      "type": "boolean",
      "default": true,
      "required": true
    },
    "isTermsAgreed": {
      "type": "boolean",
      "default": true,
      "required": true
    },
    "portfolioLink": {
      "type": "string"
    },
    "portfolioFileId": {
      "type": "string"
    },
    "extraInfo": {
      "type": "string"
    },
    "users_permissions_user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "applicants"
    },
    "talentCommunity": {
      "type": "boolean"
    },
    "notes": {
      "type": "text"
    },
    "comments": {
      "type": "text"
    },
    "resumeUrl": {
      "type": "string"
    },
    "website": {
      "type": "string"
    },
    "status": {
      "type": "string"
    },
    "opportunity": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::opportunity.opportunity"
    },
    "interests": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::interest.interest"
    }
  }
}
